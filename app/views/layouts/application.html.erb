<!DOCTYPE html>


<html lang="ru" class="no-js">
<head>
  <title><%= full_title(yield(:title)) %></title>

  <meta charset="utf-8">

  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic' rel='stylesheet' type='text/css'>

  <%= stylesheet_link_tag "/assets/laboq/bootstrap.css", media: "screen" %> 
  <%= stylesheet_link_tag "/assets/laboq/magnific-popup.css", media:"screen" %>
  <%= stylesheet_link_tag "/assets/laboq/font-awesome.css", media: "screen" %>
  <%= stylesheet_link_tag "/assets/laboq/flexslider.css", media: "screen" %>
  <%= stylesheet_link_tag "/assets/laboq/style.css", media: "screen" %>
  <%= stylesheet_link_tag "/assets/laboq/responsive.css", media: "screen" %>
  <%= stylesheet_link_tag "footer", media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag "colors", media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag "basepage", media: "all", "data-turbolinks-track" => true %>


  <%= javascript_include_tag "/assets/laboq/jquery.min.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/jquery.migrate.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/jquery.imagesloaded.min.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/jquery.isotope.min.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/jquery.magnific-popup.min.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/plugins-scroll.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/bootstrap.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/retina-1.1.0.min.js?body=1" %>
  <%= javascript_include_tag "/assets/laboq/script.js?body=1" %>
  <%= javascript_include_tag "/assets/jquery.js?body=1" %>
  <%= javascript_include_tag "/assets/jquery_ujs.js?body=1" %>
  <%= javascript_include_tag "/assets/turbolinks.js?body=1" %>
  <%= javascript_include_tag "/assets/ckeditor/config.js?body=1" %>
  <%= javascript_include_tag "/assets/ckeditor/override.js?body=1" %>
  <%= javascript_include_tag "/assets/ckeditor/init.js?body=1" %>
  <%= javascript_include_tag "/assets/ckeditor/ckeditor.js?body=1" %>

  <!--<%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>-->
  <%= javascript_include_tag "application"%>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>

</head>
<body>

  <!-- Container -->
  <div id="container">
    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |key, value| %>
        <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>
      <%= yield %>
    </div>

    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>

  </div>
  <!-- End Container -->

  <div class="preloader">
    <img alt="" src="assets/preloader.GIF">
  </div>

</body>
</html>