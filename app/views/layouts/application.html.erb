<body>

  <!-- Container -->
  <div id="container">
    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |key, value| %>
        <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>
      <%= yield %>
    </div>

    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>

  </div>
  <!-- End Container -->

  <div class="preloader">
    <img alt="" src="assets/preloader.GIF">
  </div>

</body>
